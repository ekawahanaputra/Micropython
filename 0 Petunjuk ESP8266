D4 = GPIO_2

3V = POWER 3 VOLT
G  = GROUND

D5 = GPIO_14
D6 = GPIO_12
D7 = GPIO_13
D8 = GPIO_15
RX = GPIO_3
TX = GPIO_1

G  = GROUND
3V = POWER 3 VOLT

